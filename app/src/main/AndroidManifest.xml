<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vipul.hp_hp.timeline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimeLineActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.wxxiaomi.ming.MyTimeLineActivity" />
        <activity android:name="com.wxxiaomi.ming.CustomViewActivity" />
        <activity android:name="com.com.wxxiaomi.demo.LatestActivity"></activity>
    </application>

</manifest>